<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.argeo.slc.core.test.tree">
	<class name="TreeTestResult" table="TREE_TEST_RESULT">
		<id name="tid" column="T_ID">
			<generator class="native" />
		</id>

		<many-to-one name="numericResultId" unique="true"
			not-null="true" column="TEST_RESULT_ID" cascade="all" lazy="false" />

		<map name="resultParts" table="RESULT_PARTS_MAP" cascade="all"
			lazy="false" sort="natural">
			<key column="PARENT_ID" />
			<map-key-many-to-many
				class="org.argeo.slc.core.structure.tree.TreeSPath" column="PATH" />
			<many-to-many class="PartSubList" column="PART_SUB_LIST_ID"
				unique="true" lazy="false" />
		</map>
	</class>
</hibernate-mapping>