<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.argeo.slc.core.structure.tree">
	<class name="TreeSPath" table="TREE_SPATH">
		<id name="tid" column="T_ID">
			<generator class="native" />
		</id>
		<property name="asUniqueString" column="PATH" not-null="true"
			unique="true" />
		<property name="separator" column="SEPARATOR" not-null="true" />
	</class>
</hibernate-mapping>