<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.argeo.slc.core.test.tree">
	<class name="PartSubList" table="PART_SUB_LIST">
		<id name="tid" column="T_ID">
			<generator class="native" />
		</id>
		<list name="parts" table="SUB_LIST_PARTS"  cascade="all" lazy="false">
			<key column="PARENT_ID" unique="true" />
			<list-index column="INDEX" />
			<many-to-many
				class="org.argeo.slc.core.test.SimpleResultPart" column="PART_ID" lazy="false"/>
		</list>
	</class>
</hibernate-mapping>