<lifecycles>
	<lifecycle>
		<id>eclipseTarget</id>
		<phases>
			<phase>
				<id>initialize</id>
				<executions>
					<execution>
						<goals>
							<goal>org.argeo.slc.maven:maven-dependency-argeo-plugin:unpack-dependencies</goal>
						</goals>
						<configuration>
							<useSubDirectoryPerArtifact>
								true
                            </useSubDirectoryPerArtifact>
						</configuration>
					</execution>
					<execution>
						<goals>
							<goal>eclipseTarget-collect</goal>
						</goals>
					</execution>
				</executions>
			</phase>
		</phases>
	</lifecycle>
