<lifecycles>
	<lifecycle>
		<id>env</id>
		<phases>
			<phase>
				<id>initialize</id>
				<executions>
					<execution>
						<goals>
							<goal>org.apache.maven.plugins:maven-dependency-plugin:unpack
							</goal>
						</goals>
						<configuration>
							<artifactItems>
								<artifactItem>
									<groupId>org.argeo.dep.dist</groupId>
									<artifactId>jython</artifactId>
									<version>2.5.beta0</version>
									<classifier>lib</classifier>
									<type>zip</type>
									<outputDirectory>${project.build.directory}/jython
									</outputDirectory>
									<overWrite>false</overWrite>
								</artifactItem>
								<artifactItem>
									<groupId>org.argeo.dep.dist</groupId>
									<artifactId>qooxdoo-sdk</artifactId>
									<classifier>dist</classifier>
									<version>0.8</version>
									<type>zip</type>
									<outputDirectory>${basedir}/src/main/webapp
									</outputDirectory>
									<overWrite>false</overWrite>
								</artifactItem>
							</artifactItems>
						</configuration>
					</execution>
				</executions>
			</phase>
		</phases>
	</lifecycle>
</lifecycles>
