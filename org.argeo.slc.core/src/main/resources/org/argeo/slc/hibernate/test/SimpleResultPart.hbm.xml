<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.argeo.slc.core.test">
	<class name="SimpleResultPart" table="SIMPLE_RESULT_PART">
		<id name="tid" column="T_ID">
			<generator class="native" />
		</id>
		<property name="status" column="STATUS" not-null="true" />
		<property name="message" column="MESSAGE" not-null="true" />
		<property name="exceptionMessage" column="EXCEPTION_MESSAGE"
			not-null="false" />
		<list name="exceptionStackLines" cascade="all" lazy="false">
			<key column="RESULT_PART_ID" />
			<list-index column="INDX" />
			<element type="string" column="STACK_LINE" />
		</list>
	</class>
</hibernate-mapping>