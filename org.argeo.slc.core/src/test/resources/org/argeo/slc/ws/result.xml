<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<import
		resource="classpath:/org/argeo/slc/ws/client/spring/applicationContext.xml" />

	<bean id="testResult"
		class="org.argeo.slc.core.test.tree.TreeTestResult">
		<property name="uuid">
			<bean factory-bean="uuid" factory-method="toString"></bean>
		</property>
		<property name="listeners">
			<list>
				<ref bean="resultLogger" />
				<ref bean="resultWsNotfier" />
			</list>
		</property>
	</bean>

	<bean name="resultWsNotfier"
		class="org.argeo.slc.core.test.tree.WebServiceSlcExecutionNotifier">
		<property name="template"
			ref="slcDefault.ws.client.webServiceTemplate" />
	</bean>

</beans>