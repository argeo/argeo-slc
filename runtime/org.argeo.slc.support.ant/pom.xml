		</dependency>

		<dependency>
			<groupId>org.argeo.tp</groupId>
			<artifactId>org.apache.commons.net</artifactId>
		</dependency>
		<dependency>
			<groupId>org.argeo.tp</groupId>
			<artifactId>org.apache.oro</artifactId>
		</dependency>

		<dependency>
			<groupId>org.argeo.tp</groupId>
