		<relativePath>..</relativePath>
	</parent>
	<artifactId>org.argeo.slc.support.ant</artifactId>
	<packaging>jar</packaging>
	<name>SLC Support Ant</name>
	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.felix</groupId>
				<artifactId>maven-bundle-plugin</artifactId>

