<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.argeo.slc.core.test.tree">
	<class name="PartSubList" table="PART_SUB_LIST">
		<id name="tid" column="T_ID">
			<generator class="native" />
		</id>
		<list name="parts" cascade="all" lazy="false">
			<key column="PART_SUBLIST_ID" />
			<list-index column="INDX" />
			<composite-element
				class="org.argeo.slc.core.test.SimpleResultPart">
				<property name="status" column="STATUS" not-null="true" />
				<property name="message" column="MESSAGE"
					not-null="true" />
				<property name="exceptionMessage"
					column="EXCEPTION_MESSAGE" not-null="false" />
				<property name="testRunUuid" column="TEST_RUN_UUID"
					not-null="false" />
			</composite-element>
		</list>
	</class>
</hibernate-mapping>