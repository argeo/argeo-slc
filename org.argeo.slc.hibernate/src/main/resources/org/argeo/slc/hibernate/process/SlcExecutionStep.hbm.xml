<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.argeo.slc.core.process">
	<class name="SlcExecutionStep" table="SLC_EXECUTION_STEP">
		<id name="uuid" column="UUID" />
		<property name="type" column="TYPE" />
		<property name="begin" column="BEGIN" />

		<list name="logLines" cascade="all" lazy="false"
			table="SLC_EXECUTION_STEP_LOG_LINE">
			<key column="SLC_EXECUTION_STEP_ID" />
			<list-index column="INDX" />
			<element column="LOG_LINE" type="string" />
		</list>
	</class>
</hibernate-mapping>